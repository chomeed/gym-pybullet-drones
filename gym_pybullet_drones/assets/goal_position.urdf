<robot name="obstacle">
  <link name="baseLink">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".1"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.2 0.2 0.02"/> <!-- set cube size -->
      </geometry>
      <material name="Green"> <!-- set color-->
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
  </link>
</robot>