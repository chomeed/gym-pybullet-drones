<?xml version="1.0" ?>
<robot name="plane">
  <link name="planeLink">
    <contact>
      <lateral_friction value="1"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value=".0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="plane_env2.obj" scale="1 1 1"/> <!-- Adjust the scale values -->
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -5"/> <!-- Adjust the z-coordinate accordingly -->
      <geometry>
        <box size="6 6 10"/> <!-- Adjust the size values accordingly -->
      </geometry>
    </collision>
  </link>
</robot>