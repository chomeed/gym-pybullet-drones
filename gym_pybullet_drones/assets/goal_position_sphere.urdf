<robot name="obstacle">
  <link name="baseLink">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".1"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.15"/>
      </geometry>
      <material name="Red"> <!-- set color-->
        <color rgba="1 0 0 0.5"/>
      </material>
    </visual>
  </link>
</robot>